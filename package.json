{
  "name": "fetch-captain",
  "version": "0.0.3",
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "license": "MIT",
  "author": "XLor",
  "main": "lib/main.js",
