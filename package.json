    "nodemailer": "^6.7.7"
  },
  "devDependencies": {
    "@types/markdown-it": "12.2.3",
    "@types/node": "18.0.3",
    "@types/nodemailer": "6.4.5",
    "@vercel/ncc": "0.34.0",
    "execa": "6.1.0",
    "fs-extra": "10.1.0",
    "js-yaml": "4.1.0",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  },
  "packageManager": "pnpm@7.13.3"
