{
  "name": "fetch-captain",
  "version": "0.0.3",
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "license": "MIT",
  "author": "XLor",
  "main": "lib/main.js",
  "scripts": {
    "all": "npm run build && npm run format && npm run lint && npm run package",
    "build": "tsc",
    "format": "prettier --write ./src/*.ts",
    "format-check": "prettier --check ./src/*.ts",
    "lint": "eslint ./src/*.ts",
    "package": "ncc build --license licenses.txt",
    "release": "node ./scripts/release.js"
  },
  "dependencies": {
    "@actions/core": "^1.6.0",
    "axios": "^0.26.0",
    "date-fns": "^2.28.0",
    "markdown-it": "^12.3.2",
    "nodemailer": "^6.7.2"
  },
  "devDependencies": {
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^16.10.5",
    "@types/nodemailer": "^6.4.4",
    "@typescript-eslint/parser": "^5.8.1",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.3.2",
    "execa": "^6.1.0",
    "fs-extra": "^10.0.1",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4"
  }
}
